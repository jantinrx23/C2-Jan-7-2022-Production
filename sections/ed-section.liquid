{% for block in section.blocks %}
	{% if block.type == 'price-text-banner' %}
		{% include 'price-text-banner'%}
	{% endif %}

	{% if block.type == 'phone-cta' %}
		{% include 'phone-cta' %}
	{% endif %}

	{% if block.type == 'text-with-search-section' %}
		{% include 'text-with-search-section' %}
	{% endif %}

	{% if block.type == 'how-possible' %}
		{% include 'how-possible' %}
	{% endif %}
	
	{% if block.type == 'logos-with-text' %}
		{% include 'text-with-logos-section' %}
	{% endif %}

	{% if block.type == 'list-with-bgimage' %}
		{% include 'list-with-bg-image' %}
	{% endif %}

	{% if block.type == 'ed-column-content' %}
		{% include 'ed-column-content' %}
	{% endif %}
	

{% endfor %}



{% schema %}
  {
    "name": "ED Page",
    "settings": [
		{
          "type": "text",
          "id": "heading",
          "label": "Page Heading"
		}
	],
	"blocks": [
      {
          "type": "price-text-banner",
          "name": "Price Text Banner",
          "settings": [
              {
                  "type": "image_picker",
                  "id": "background_image",
                  "label": "Background Image"
              },
              {
                  "type": "header",
                  "content": "Left Text"
              },
              {
                  "type": "text",
                  "id": "heading",
                  "label": "Heading",
                  "default": "TIN Rx"
              },
              {
                  "type": "text",
                  "id": "price",
                  "label": "Price",
                  "default": "$5"
              },
              {
                  "type": "textarea",
                  "id": "price_label",
                  "label": "Label",
                  "default": "30 days of Astrovastatin (Lipitor)."
              },
              {
                  "type": "checkbox",
                  "id": "show_check",
                  "label": "Show Check",
                  "default": true
              },
              {
                  "type": "header",
                  "content": "Right Text"
              },
              {
                  "type": "text",
                  "id": "right_heading",
                  "label": "Heading",
                  "default": "Chain Pharmacy"
              },
              {
                  "type": "text",
                  "id": "right_price",
                  "label": "Price",
                  "default": "$91"
              },
              {
                  "type": "textarea",
                  "id": "right_price_label",
                  "label": "Label",
                  "default": "30 days of Lipitor."
              },
              {
                  "type": "checkbox",
                  "id": "right_show_check",
                  "label": "Show Check",
                  "default": false
              },
              {
                  "type": "textarea",
                  "id": "disclaimer_text",
                  "label": "Disclaimer Text",
                  "default": "*Data based on average retail price of atorvastatin (Lipitor), subject to change"
              }
          ]
      },
      {
          "type": "phone-cta",
          "name": "Phone Call to action",
          "settings": [
            {
              "type": "select",
              "id": "color_mode",
              "label": "Select Color Mode",
              "options": [
                  {
                  "value": "light-mode","label": "Light Mode"
                  },
                  {
                  "value": "dark-mode","label": "Dark Mode"
                  }
				]
            },
            {
              "type": "text",
              "id": "heading",
              "label": "Heading 1",
              "default": "Not sure how to proceed?"
            },
            {
              "type": "text",
              "id": "heading_2",
              "label": "Heading 2",
              "default": "Talk to a TIN pharmacy specialist."
            },
            {
              "type": "text",
              "id": "heading_3",
              "label": "Heading 3",
              "default": "We're ready to take your call."
            },
            {
              "type": "text",
              "id": "cta_btn_title",
              "label": "Button Title",
              "default": "(209) 850-8828"
            },
            {
              "type": "url",
              "id": "cta_btn_url",
              "label": "Button Link"
            },
			{
              "type": "text",
              "id": "cta_note",
              "label": "Special Note",
				"default": "Specialist are available 9am-5pm PST."
			}
        ]
      },      
	  {
          "type": "text-with-search-section",
          "name": "Text With Search",
          "settings": [
            {
              "type": "text",
              "id": "search_title",
              "label": "Title",
              "default": "Browse our $5 Dollar for 30 Day Prescriptions"
            }
        ]

      },      
	  {
          "type": "how-possible",
          "name": "How possible",
          "settings": [
            {
              "type": "text",
              "id": "hp_heading",
              "label": "Heading",
              "default": "How is that even possible?"
            },
            {
              "type": "image_picker",
              "id": "icon_image_1",
              "label": "Icon Image 1"
            },
			{
              "type": "text",
              "id": "icon_title_1",
              "label": "Title 1",
              "default": "DRUG MANUFACTURER"
            },
            {
              "type": "image_picker",
              "id": "icon_image_2",
              "label": "Icon Image 2"
            },
			{
              "type": "text",
