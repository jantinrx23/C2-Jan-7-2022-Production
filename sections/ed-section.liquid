        "name": "List with Image",
		"settings": [
			{
              "type": "select",
              "id": "text-alignment",
              "label": "Select Text Alignment",
              "options": [
                  {
                  "value": "left-text","label": "Left"
                  },
                  {
                  "value": "right-text","label": "Right"
                  }
				]
            },
			{
                "type": "image_picker",
                "id": "background_image",
                "label": "Background Image"
          	},
            {
                "type": "richtext",
                "id": "heading",
                "label": "Heading",
				"default": "<p>Our $2 dollar Sildenafil is cheaper than a nice cup of coffee.</p>"
            },
            {
                "type": "text",
                "id": "text_1",
